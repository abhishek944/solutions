#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

int main(int argc, const char * argv[]) {
    
    int t;
    cin >> t;
    while(t--) {
        string a, b;
        cin >> a >> b;
        
        reverse(a.begin(), a.end());
        reverse(b.begin(), b.end());
        
        int A = stoi(a);
        int B = stoi(b);
        
        string ans = to_string(A + B);
        reverse(ans.begin(), ans.end());
        cout << stoi(ans) << endl;
    }
}
