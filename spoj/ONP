#include <iostream>
#include <algorithm>
#include <vector>
#include <stack>

using namespace std;

int getPriority(char c) {
    if(c == '(') return 10;
    if(c == ')') return 20;
    if (c == '+') return 3;
    if (c == '-') return 4;
    if (c == '*') return 5;
    if (c == '/') return 6;
    if (c == '^') return 7;
    return 2;
}

int main(int argc, const char * argv[]) {
    
    int t;
    cin >> t;
    
    while(t--) {
        string a;
        cin >> a;
        
        stack < char > s;
        
        vector < char > ans;
        for(int i = 0;i < a.size();i++) {
            if (a[i] == '(') {
                s.push(a[i]);
            } else if (a[i] == ')') {
                while(s.top() != '(') {
                    ans.push_back(s.top());
                    s.pop();
                }
                s.pop(); // '('
            } else {
                while(!s.empty() && (getPriority(a[i]) > getPriority(s.top()))) {
                    ans.push_back(s.top());
                    s.pop();
                }
                s.push(a[i]); // a[i]
            }
        }
        
        for(int i = 0;i < ans.size();i++) cout << ans[i]; cout << endl;
    }
    
    return 0;
}
