#include <iostream>
#include <algorithm>
#include <vector>
#include <stack>
#include <map>

using namespace std;

long long dp[5234];

int main(int argc, const char * argv[]) {

    string a;
    while(cin >> a) {
        if ( a== "0") break;
        
        bool invalid = false;
        
        dp[0] = 1;
        for(int i = 0;i < a.size(); i++) {
            if (i == 0) {
                dp[1] = 1;
            } else {
                if (a[i] == '0' && (a[i - 1] != '1' && a[i - 1] != '2')) {
                    invalid = true;
                    break;
                } else if (a[i] == '0') {
                    dp[i + 1] = dp[i - 1];
                } else if (a[i - 1] == '0') {
                    dp[i + 1] = dp[i];
                } else {
                    dp[i + 1] = dp[i];
                    int biVal = stoi(a.substr(i - 1, 2));
                    if (biVal <= 26) dp[i + 1] += dp[i - 1];
                }
            }
           // cout << i << " " << dp[i + 1] << endl;
        }
        if(invalid) cout << 0 << endl;
        else cout << dp[a.size()]<< endl;
    }
    
    return 0;
}
